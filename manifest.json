{
	"manifest_version": 3,
	"name": "HYPR_RME",
	"description": "Automate APM",
	"version": "1.0",
	"icons": {
		"16": "images/icon.png",
		"32": "images/icon.png",
		"48": "images/icon.png",
		"128": "images/icon.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["activeTab", "scripting", "tabs", "webNavigation"],
	"host_permissions": [
    		"http://*/*",
	    	"https://*/*"
  	],
	"action":{
		"default_popup": "popup.html"
	},
	  "commands": {
    		"_execute_action": {
      		"suggested_key": {
        	"default": "Ctrl+B",
        	"mac": "Command+B"
      }
    }
  }

}
